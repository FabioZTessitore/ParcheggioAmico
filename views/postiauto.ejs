<!-- INIZIO CONTAINER CON JUMBOTRON -->
<div class="container mt-5">

  <div class="jumbotron">
    <h4 class="display-6 text-center">Parcheggio Amico</h4>
    <h1 class="display-4 text-center"><a href="/parcheggi/<%= parcheggio._id %>"><%= parcheggio.name %></a></h1>
  </div>







  <form action="/postiauto" method="POST" class="form-inline">
    <div class="form-group mx-sm-3 mb-2">

      <input type="hidden" class="form-control" name="id" value="<%= parcheggio._id%>" >
      
      <label for="inputNomeP" class="sr-only">Nome Parcheggio</label>
      <input type="text" class="form-control" id="inputNomeP" name="inputNomeP" placeholder="Nome Posto">
    </div>
    <button type="submit" class="btn btn-primary mb-2">Salva Parcheggio</button>
  </form>

<table class="table table-bordered table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Parcheggio ID</th>
      <th scope="col">Nome Parcheggio</th>
      <th scope="col">Occupato</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
<% listaposti.forEach(function(postoauto){ %>
    <tr>
      <td scope="row"><%= postoauto._id %></td>
      <td><%= postoauto.parcheggioID %></td>
      <td><%= postoauto.name %></td>
      <td><%= postoauto.occupato %></td>
      <td class="text-center">
        <form action="#" method="POST" class="form-inline">
          <input type="hidden" name="id" value="">
          <button type="submit" class="btn btn-danger">&times;</button>
        </form>
      </td>

    </tr>
    <% }); %>
  </tbody>
</table>


</div>



















